{
    "name": "noetic/nitseditor",
    "type": "project",
    "description": "NitsEditor: A product of Noetic IT Services Pvt. Ltd.",
    "keywords": [
        "nitseditor",
        "framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nitish Kumar",
            "email": "nitishkumar@noeticitservices.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "nitseditor/framework": "dev-master"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "repositories": [ {
        "type": "package",
        "package": {
            "name": "nitseditor/framework",
            "version": "dev-master",
            "source": {
                "url": "http://35.155.242.12:7990/scm/nit/framework.git",
                "type": "git",
                "reference": "master"
            }
        }
    }],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "secure-http": false
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Noetic\\Plugins\\": "plugins/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
